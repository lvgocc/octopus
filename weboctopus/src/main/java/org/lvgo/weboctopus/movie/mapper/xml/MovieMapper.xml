<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.lvgo.weboctopus.movie.mapper.MovieMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.lvgo.weboctopus.movie.entity.Movie">
        <id column="movie_id" property="movieId"/>
        <result column="movie_name" property="movieName"/>
        <result column="initial_release_date" property="initialReleaseDate"/>
        <result column="movie_time" property="movieTime"/>
        <result column="producer_country" property="producerCountry"/>
        <result column="source" property="source"/>
        <result column="data_time" property="dataTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        movie_id, movie_name, initial_release_date, movie_time, producer_country, source, data_time
    </sql>

</mapper>
